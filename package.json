{
  "name": "norma",
  "version": "0.2.6",
  "description": "A function argument organizer",
  "main": "norma.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha test/*.test.js",
    "build": "./node_modules/.bin/pegjs norma-parser.pegjs",
    "jshint": "./node_modules/.bin/jshint norma.js",
    "docco": "./node_modules/.bin/docco norma.js -o doc",
    "gh-pages-doc": "cp -r doc/* ../gh-pages/norma/doc"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/rjrodger/norma.git"
  },
  "keywords": [
    "norma"
  ],
  "author": "Richard Rodger (http://richardrodger.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rjrodger/norma/issues"
  },
  "homepage": "https://github.com/rjrodger/norma",
  "devDependencies": {
    "pegjs": "~0.8.0",
    "mocha": "~1.17.1",
    "docco": "~0.6.3",
    "jshint": "~2.5.0"
  },
  "dependencies": {
    "lodash": "~2.4.1",
    "eraro": "~0.4.1"
  }
}
