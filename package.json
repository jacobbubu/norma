{
  "name": "norma",
  "version": "1.0.0",
  "description": "A function argument organizer",
  "main": "norma.js",
  "scripts": {
    "test": "./test.sh",
    "build": "./build.sh",
    "clean": "rm -rf node_modules yarn.lock package-lock.json",
    "repo-tag": "REPO_VERSION=`node -e \"console.log(require('./package').version)\"` && echo TAG: v$REPO_VERSION && git commit -a -m v$REPO_VERSION && git push && git tag v$REPO_VERSION && git push --tags",
    "repo-publish": "npm run repo-tag && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/rjrodger/norma.git"
  },
  "keywords": [
    "norma",
    "function",
    "method",
    "parameter",
    "argument",
    "parameters",
    "arguments",
    "type",
    "signature"
  ],
  "author": "Richard Rodger (http://richardrodger.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rjrodger/norma/issues"
  },
  "homepage": "https://github.com/rjrodger/norma",
  "devDependencies": {
    "docco": "^0.8.0",
    "jshint": "^2.10.2",
    "mocha": "^6.2.0",
    "pegjs": "^0.10.0",
    "uglify-js": "^3.6.0"
  },
  "dependencies": {
    "eraro": "^1.1.0",
    "lodash": "^4.17.15"
  },
  "files": [
    "README.md",
    "LICENSE.txt",
    "norma.js",
    "norma-parser.js",
    "norma-min.js",
    "norma-min.js.map"
  ]
}
